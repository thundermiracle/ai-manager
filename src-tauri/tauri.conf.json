{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "AI Manager",
  "version": "0.1.0",
  "identifier": "com.thundermiracle.ai-manager",
  "build": {
    "beforeDevCommand": "pnpm run generate:tauri-icons && pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm run generate:tauri-icons && pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "AI Manager",
        "width": 1200,
        "height": 820
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "icon": [
      "generated-icons/32x32.png",
      "generated-icons/128x128.png",
      "generated-icons/128x128@2x.png",
      "generated-icons/icon.icns",
      "generated-icons/icon.ico"
    ]
  }
}
